<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#90a4ae"
    tools:context=".Datos">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:layout_marginBottom="@dimen/activity_vertical_margin">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="Datos Importantes"
        android:textColor="@color/TextN"
        android:layout_margin="@dimen/activity_vertical_margin"
        android:textSize="20sp"
        android:textStyle="bold" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="Programacion de Dispositivos Moviles"
        android:layout_margin="@dimen/activity_vertical_margin"
        android:textColor="@color/TextN"
        android:textSize="20sp"
        android:textStyle="normal" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="Evaluacion 2 "
        android:textColor="@color/TextN"
        android:layout_margin="@dimen/activity_vertical_margin"
        android:textSize="20sp"
        android:textStyle="normal" />
    <TextView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal"
    android:text="Leonardo Eliseo Hernandez Sandoval"
    android:layout_margin="@dimen/activity_vertical_margin"
    android:textColor="@color/TextN"
    android:textSize="20sp"
    android:textStyle="normal" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="HS16-I04-001"
        android:textColor="@color/TextN"
        android:layout_margin="@dimen/activity_vertical_margin"
        android:textSize="20sp"
        android:textStyle="normal" />
    </LinearLayout>

</LinearLayout>